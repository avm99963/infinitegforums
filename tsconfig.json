{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "module": "es2022",
    "target": "es2022",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "bazel-bin",
    "bazel-infinitegforums",
    "bazel-out",
    "bazel-testlogs",
    "bazel-*"
  ]
}
