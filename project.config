[access]
	inheritFrom = All-Projects
[submit]
	action = inherit
[project]
	description = Chrome extension which implements several enhancements in the Google Forums
[access "refs/*"]
	read = group GitHub Replication
	push = +force group user/Adrià Vilanova Martínez (avm99963)
[commentlink "GitHub"]
  match = "(?:[Bb]ug|[Ii]ssue|[Cc]lose[sd]?|[Ff]ix(?>es|ed)?|[Rr]esolve[sd]?):\\s+#(\\S+)"
  link = https://github.com/avm99963/infinitegforums/issues/$1
