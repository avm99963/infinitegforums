      <trans-unit id="bulkReportControls.abuseChip.defaultLabel" xml:space="preserve">
        <source>Mark as abuse</source>
        <target state="translated">تحديد كإساءة</target>
        <note from="lit-localize">Chip shown to report a reply in a thread.</note>
      </trans-unit>
