        <source>Mark as abuse</source>
        <target state="translated">Отметить как нарушение</target>
        <note from="lit-localize">Chip shown to report a reply in a thread.</note>
      </trans-unit>
      <trans-unit id="bulkMove.progressDialog.status.error" xml:space="preserve">
        <source>Error</source>
        <target state="translated">Ошибка</target>
        <note from="lit-localize">Status of a thread which has been attempted to move, but failed.</note>
